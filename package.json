{
  "name": "compose",
  "description": "NOpe",
  "author": "Kasper Bartholdi Gustavii <kasper.gustavii@live.se>",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "build": "npm run clean && better-npm-run build",
    "clean": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "lint": "eslint -c .eslintrc src",
    "start": "better-npm-run start",
    "test": "better-npm-run test",
    "test:ci": "npm run clean:coverage && better-npm-run test:ci",
    "test:jenkins": "mkdirp reports && npm run test:prod && sed \"s#SF:$PWD#SF:/data#g\" coverage/report-lcov/lcov.info > reports/karma.info"
  },
  "betterScripts": {
    "build": {
      "command": "webpack -p --config webpack/webpack.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test:ci": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start": {
      "command": "babel-node webpack/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "css-loader": "^0.28.11",
    "cors": "2.8.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jasmine": "^2.9.3",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.2.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^v0.0.32",
    "karma-webpack": "^3.0.0",
    "node-sass": "^4.8.3",
    "phantomjs-prebuilt": "^2.1.16",
    "popper.js": "^1.14.3",
    "postcss-loader": "^2.1.4",
    "react-hot-loader": "^4.0.1",
    "redux-mock-store": "^1.5.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.1",
    "sinon": "^4.5.0",
    "style-loader": "^0.20.3",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "url-loader": "^1.0.1",
    "webpack": "^3.12.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.2"
  }
}
